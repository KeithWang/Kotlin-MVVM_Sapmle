<?xml version="1.0" encoding="utf-8"?>
<demo.vicwang.mvvm.utility.custom.SlideRightRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:clickable="true"
    android:focusable="true"
    tools:context=".activity.MainActivity">

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@drawable/actionbar_shadow" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/house_main_recycleview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</demo.vicwang.mvvm.utility.custom.SlideRightRelativeLayout>